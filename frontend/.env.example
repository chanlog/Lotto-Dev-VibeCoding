# 프론트엔드 환경 변수 예시

# API 기본 URL
VUE_APP_API_BASE_URL=http://localhost:8000/api

# 애플리케이션 설정
VUE_APP_NAME="로또 분석 서비스"
VUE_APP_VERSION=1.0.0

# 개발 환경 설정
NODE_ENV=development
VUE_APP_ENV=development

# API 키 (동행복권 API 등)
VUE_APP_LOTTO_API_KEY=your_api_key_here

# 구글 애널리틱스
VUE_APP_GA_TRACKING_ID=your_ga_tracking_id_here

# 기타 외부 서비스
VUE_APP_SENTRY_DSN=your_sentry_dsn_here